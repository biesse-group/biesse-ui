  "scripts": {
    "test": "jest",
    "build": "rimraf dist && rollup -c",
    "postbuild": "node scripts/transform-imports.cjs",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "npm run storybook -- --no-open --quiet --ci",
    "build-storybook": "rimraf storybook-static && storybook build",
    "eslint": "eslint -c .eslintrc.json --max-warnings 0 .",
    "prettier-check": "prettier . --check",
    "prettier-fix": "prettier . --write",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook:ci\" \"wait-on tcp:6006 && npm run test-storybook\"",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "local-package": "rimraf *.tgz && npm run build && npm pack"
  },
  "main": "dist/index.js",
